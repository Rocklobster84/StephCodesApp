{
  "name": "steph-codes-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon -r dotenv/config app.js",
    "build": "cd client && npm install && npm run build",
    "db:create": "npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:g:migration": "npx sequelize-cli migration:generate --name",
    "db:reset": "npx sequelize-cli db:drop && sequelize-cli db:create && sequelize-cli db:migrate",
    "db:g:seed": "npx sequelize-cli seed:generate --name",
    "db:seeds": "npx sequelize-cli db:seed:all",
    "db:reseed": "npx sequelize-cli db:drop && sequelize-cli db:create && sequelize-cli db:migrate && sequelize-cli db:seed:all"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "http-errors": "~2.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "nodemon": "^3.1.4",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "postgres": "^3.4.4",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2",
    "serve-favicon": "^2.5.0",
    "sqlite3": "^5.1.7",
    "steph-codes-app": "file:"
  },
  "devDependencies": {
    "eslint-config-prettier": "^9.1.0",
    "prettier": "3.3.3"
  }
}
